<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>registration-app</display-name>
  <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
   
  </welcome-file-list>
  <servlet>
  <servlet-name>registerHere</servlet-name>
  <servlet-class>com.xworkz.Registration.Servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>registerHere</servlet-name>
  <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>registerhere</servlet-name>
  <servlet-class>com.xworkz.Registration.Servlets.EnteredDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>registerhere</servlet-name>
  <url-pattern>/registration</url-pattern>
  </servlet-mapping>
</web-app>